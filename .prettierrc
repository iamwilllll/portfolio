{
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "es5",
    "printWidth": 130,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}
